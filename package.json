{
  "name": "sonrix-voice-chat",
  "version": "1.0.0",
  "description": "Professional WebRTC voice chat platform - Ubuntu 22.04 LTS Optimized",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "production": "NODE_ENV=production node server.js",
