    "mysql2": "3.6.3",
    "express-session": "1.17.3",
    "express-rate-limit": "6.10.0",
    "helmet": "7.1.0",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "compression": "1.7.4"
  },
  "devDependencies": {
    "nodemon": "3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "linux"
  ],
  "keywords": [
    "webrtc",
    "voice-chat",
    "admin-panel", 
    "mysql8",
